{
  "name": "lifelight-gemma3n-challenge",
  "version": "1.0.0",
  "private": true,
  "description": "LifeLight Gemma 3N Challenge Submission - Multi-platform AI-powered medication tracking app",
  "scripts": {
    "dev": "next dev",
    "build": "npm run build:check && next build",
    "build:prod": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "build:check": "tsx scripts/build-check.ts",
    "download:gemma-models": "tsx scripts/download-gemma-models.ts",
    "init:capacitor": "npx cap init",
    "add:android": "npx cap add android",
    "build:android": "npm run build && npx cap copy android && npx cap run android",
    "dev:android": "npx cap run android --livereload --external",
    "sync:android": "npx cap sync android",
    "open:android": "npx cap open android",
    "setup": "npm install && npm run download:gemma-models"
  },
  "dependencies": {
    "@capacitor/android": "^6.0.0",
    "@capacitor/cli": "^7.4.2",
    "@capacitor/core": "^6.0.0",
    "@mlc-ai/web-llm": "^0.2.79",
    "@xenova/transformers": "^2.17.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "dexie": "^4.0.11",
    "framer-motion": "^12.23.12",
    "lucide-react": "^0.533.0",
    "next": "15.4.4",
    "next-pwa": "^5.6.0",
    "quagga": "^0.12.1",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-webcam": "^7.2.0",
    "tailwind-merge": "^3.3.1"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@testing-library/jest-dom": "^6.6.4",
    "@testing-library/react": "^16.3.0",
    "@types/fs-extra": "^11.0.4",
    "@types/jest": "^30.0.0",
    "@types/node": "^20",
    "@types/papaparse": "^5.3.16",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.4.4",
    "fs-extra": "^11.3.0",
    "fuse.js": "^7.1.0",
    "jest": "^30.0.5",
    "jest-environment-jsdom": "^30.0.5",
    "papaparse": "^5.5.3",
    "tailwindcss": "^4",
    "ts-jest": "^29.4.1",
    "tsx": "^4.20.3",
    "tw-animate-css": "^1.3.6",
    "typescript": "^5"
  },
  "keywords": [
    "healthcare",
    "ai",
    "gemma3n",
    "medication-tracking",
    "accessibility",
    "elderly-care",
    "offline-ai",
    "pwa",
    "android",
    "jetson"
